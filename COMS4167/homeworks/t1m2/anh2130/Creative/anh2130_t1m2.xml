    <particle m="1.0" px="10.0" py="10.0" vx="0.0" vy="0.0" fixed="1" raidus="0.02"/>
    <particle m="1.0" px="-10.0" py="10.0" vx="0.0" vy="0.0" fixed="1" raidus="0.02"/>
